{
  "compilerOptions": {
    "moduleResolution": "NodeNext",
    "importsNotUsedAsValues": "error",
    "exactOptionalPropertyTypes": true,
    "target": "ESNext",
    "declaration": true,
    "module": "NodeNext",
    "outDir": "./lib",
    "strict": true,
    "noImplicitAny": true
  },
  "include": ["src"]
}
